@page "/calender"
@using Syncfusion.Blazor.Schedule
<HeadContent>    <link href="_content/Syncfusion.Blazor.Themes/bootstrap5.css" rel="stylesheet" /></HeadContent>
<SfSchedule TValue="object" ShowWeekend="false" StartHour="07:00" EndHour="18:00" Height="950px">
    <ScheduleViews>
        <!-- Day View -->
        <ScheduleView Option="View.Day" StartHour="08:00" EndHour="12:00" ShowWeekend="false" MaxEventsPerRow="10"></ScheduleView>

        <!-- Week View -->
        <ScheduleView Option="View.Week" IsSelected="true" MaxEventsPerRow="10"></ScheduleView>

        <!-- Agenda View -->
        <ScheduleView Option="View.Agenda"></ScheduleView>

        <!-- Month View -->
        <ScheduleView Option="View.Month"></ScheduleView>
    </ScheduleViews>

    <!-- Toolbar for View Switching -->
   
</SfSchedule>
<script src="_content/Syncfusion.Blazor.Core/scripts/syncfusion-blazor.min.js" type="text/javascript"></script>
@code {
    View CurrentView = View.Week;
    public int[] WorkingDays { get; set; } = { 1, 3, 5 };
    public class AppointmentData
    {
        
    }
}

